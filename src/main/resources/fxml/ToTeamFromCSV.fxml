<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ToTeamFromCSVController">
   <children>
      <Button fx:id="backButton" layoutX="545.0" layoutY="32.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="25.0" prefWidth="42.0" style="-fx-background-color: #3ABEF9;" text="Back" />
      <TableView fx:id="TableView" layoutY="76.0" prefHeight="324.0" prefWidth="601.0">
        <columns>
            <TableColumn fx:id="displayName" prefWidth="149.5" text="displayName" />
          <TableColumn fx:id="userId" prefWidth="149.5" text="userId" />
          <TableColumn fx:id="email" prefWidth="149.5" text="email" />
            <TableColumn fx:id="roles" prefWidth="149.5" text="roles" />
        </columns>
      </TableView>
      <Button fx:id="ChoosefilesButton" layoutX="20.0" layoutY="33.0" mnemonicParsing="false" onAction="#ChoosefilesButtonClicked" prefHeight="25.0" prefWidth="89.0" style="-fx-background-color: #F19ED2;" text="Choose files" textFill="#bd1936">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <TextArea fx:id="TextArea" layoutX="125.0" layoutY="26.0" prefHeight="25.0" prefWidth="409.0" />
   </children>
</AnchorPane>
